package com.app;

public class LinearSearchArray {
//	private int[] arr;
	private int key;
	
	public LinearSearchArray(int[] arr, int key) {
		super();
//		this.arr = arr;
		this.key = key;
	}
	
	public int getKey() {
		return key;
	}
	public void setKey(int key) {
		this.key = key;
	}
	
	public void insert(int []arr,int element,int size) {
		for(int i=0;i<size-1;i++) {
			arr[i] = element;
		}
	}
}
