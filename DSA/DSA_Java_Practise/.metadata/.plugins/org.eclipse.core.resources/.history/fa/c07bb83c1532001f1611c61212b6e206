import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter size");
	int s = sc.nextInt();
	int arr[] = new int[s];
	for(int i=0;i<s;i++) {
		System.out.println("enter array element");
		arr[i] =  sc.nextInt();
	}

	
	}

}
