package sll;

public class Main2 {

	public static void main(String[] args) {
		CLL cl = new CLL();
		cl.insert(12);
		cl.insert(38);
		cl.insert(70);
		cl.insert(100, 2);
		cl.insert(20, -1);
		cl.display();
		System.out.println("--------delete by val -----");
		cl.delete(12,0);
		cl.delete(70,2);
		cl.display();
	}

}
