package com.app;

public class Main {

	public static void main(String[] args) {
		BST bst = new BST();
		bst.insert(89);
		bst.insert(45);
		bst.insert(10);
		bst.insert(65);
		bst.insert(82);
		bst.inOrder(800);
		bst.insert(10);
		System.out.println("Root ");
		bst.inOrder(bst.getRoot());
//		bst.display();
		System.out.println("Min "+bst.getMin());
		
	}

}
