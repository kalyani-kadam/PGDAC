package com.app.utils;

import java.time.LocalDate;
import java.util.Map;

import com.app.entity.Item;

public class Utils {
	public static Item addNewItem(String desc, double price, String sdate,Map<String,Item>item) {
		LocalDate shipmentDate = LocalDate.parse(sdate);
		return new Item(desc,price,shipmentDate);
	}
}
